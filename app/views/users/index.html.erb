<p style="color: green"><%= notice %></p>

<div class="container-fluid">
  <div class="d-flex justify-content-between align-items-center">
    <h1>Users</h1>
    <%= link_to "New user", new_user_path , class: "btn btn-outline-info" %>
  </div>
    <table class="table table-dark table-striped table-hover" style="text-align: center;">
      <thead>
        <tr>
        <th>deviceId</th>
        <th>deviceType</th>
        <th>deviceName</th>
        <th>socialType</th>
        <th>socialId</th>
        <th>socialToken</th>
        <th>socialEmail</th>
        <th>socialName</th>
        <th>socialImgurl</th>
        <th>advertisingId</th>
        <th>versionName</th>
        <th>versionCode</th>
        <th>utmSource</th>
        <th>utmMedium</th>
        <th>utmTerm</th>
        <th>utmContent</th>
        <th>utmCampaign</th>
        <th>Show</th>
        <th>Edit</th>
        <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <%= render user %>
            <td><%= link_to "Show", user , class: "btn btn-outline-info" %></td>
            <td><%= link_to "Edit", edit_user_path(user), class: "btn btn-outline-warning"  %> </td>
            <td><%= button_to "Destroy", user, method: :delete, class: "btn btn-outline-danger"  %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <div class="d-flex justify-content-between">
      <span><%= will_paginate @users,renderer: WillPaginate::ActionView::BootstrapLinkRenderer %></span>
      <span><%= page_entries_info(@users)%></span>
    </div>
</div>

